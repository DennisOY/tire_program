/* components/table.wxss */

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 60rpx;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #eee;
}
.bg-gr{
  color: white;
  background: rgb(79, 190, 138);
}
.th {
  width: 40%;
  justify-content: center;
  background: white;
  color: rgb(79, 190, 138);
  display: flex;
  height: 60rpx;
  align-items: center;
}